{
    "name": "9work/password-validator",
    "description": "Project to validate passwords",
    "type": "project",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpunit/php-code-coverage": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
